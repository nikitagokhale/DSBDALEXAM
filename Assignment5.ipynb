    "import pandas as pd\n",
    "\n",
    "# Load the dataset\n",
    "df = pd.read_csv('melb_data.csv')\n",
    "\n",
    "# Calculate summary statistics\n",
    "summary_stats = df.groupby('Suburb')['Price'].agg(['mean', 'median', 'min', 'max', 'std']).reset_index()\n",
    "\n",
    "# Print the summary statistics\n",
    "print(summary_stats)"
